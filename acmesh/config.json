{
  "name": "Acme.sh",
  "version": "1.0.14",
  "slug": "acmesh",
  "description": "Full ACME client to generate and renew a SSL certificate",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "map": ["ssl:rw"],
  "boot": "auto",
  "ports": { "80/tcp": "80" },
  "options": {
    "domain": null,
    "server": null,
    "cabundle": null,
    "args": null,
    "fullchainfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "domain": "str",
    "server": "str",
    "cabundle": "str",
    "args": "str?",
    "fullchainfile": "str",
    "keyfile": "str"
  }
}
